{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"units": ["rem"]
		},
		"custom": {
			"spacing": {
				"baseline": "0.25rem",
				"small": "0.5rem",
				"medium": "1rem",
				"large": "1.5rem",
				"xlarge": "2.5rem"
			}
		},
		"color": {
			"defaultPalette": false,
			"defaultGradients": false,
			"defaultDuotone": false,
			"palette": [
				{
					"slug": "primary",
					"color": "#9C27B0",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#E91E63",
					"name": "Secondary"
				},
				{
					"slug": "white",
					"color": "#FFFFFF",
					"name": "White"
				},
				{
					"slug": "neutral-0",
					"color": "#FAFAFA",
					"name": "Neutral 0"
				},
				{
					"slug": "neutral-1",
					"color": "#F5F5F5",
					"name": "Neutral 1"
				},
				{
					"slug": "neutral-2",
					"color": "#EEEEEE",
					"name": "Neutral 2"
				},
				{
					"slug": "neutral-3",
					"color": "#E0E0E0",
					"name": "Neutral 3"
				},
				{
					"slug": "neutral-4",
					"color": "#BDBDBD",
					"name": "Neutral 4"
				},
				{
					"slug": "neutral-5",
					"color": "#757575",
					"name": "Neutral 5"
				},
				{
					"slug": "neutral-6",
					"color": "#616161",
					"name": "Neutral 6"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "'Roboto', sans-serif",
					"slug": "roboto",
					"name": "Roboto",
					"fontFace": [
						{
							"fontFamily": "'Roboto', sans-serif",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src":["file:./assets/fonts/Roboto-Regular.ttf"]
						},
						{
							"fontFamily": "'Roboto', sans-serif",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src":["file:./assets/fonts/Roboto-Bold.ttf"]
						}
					]
				},
				{
					"fontFamily": "'Roboto Mono', monospace",
					"slug": "roboto-mono",
					"name": "Roboto Mono",
					"fontFace": [
						{
							"fontFamily": "'Roboto Mono', monospace",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src":["file:./assets/fonts/RobotoMono-Regular.ttf"]
						},
						{
							"fontFamily": "'Roboto Mono', monospace",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src":["file:./assets/fonts/RobotoMono-Bold.ttf"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"size": "0.875rem",
					"slug": "small",
					"name": "Small"
				},
				{
					"size": "clamp(1rem, 1rem + ((1vw - 0.48rem) * 0.24), 1.125rem)",
					"slug": "normal",
					"name": "Normal"
				},
				{
					"size": "clamp(1.125rem, 1.125rem + ((1vw - 0.48rem) * 0.24), 1.25rem)",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"size": "clamp(1.25rem, 1.25rem + ((1vw - 0.48rem) * 0.481), 1.5rem)",
					"slug": "large",
					"name": "Large"
				},
				{
					"size": "clamp(1.375rem, 1.375rem + ((1vw - 0.48rem) * 1.202), 2rem)",
					"slug": "x-large",
					"name": "X-Large"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"lineHeight": "1.5",
			"fontSize": "0.875rem"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-mono)",
					"lineHeight": "1"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-mono)",
					"lineHeight": "1"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-mono)",
					"lineHeight": "1"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-mono)",
					"lineHeight": "1.3"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-mono)",
					"lineHeight": "1.3"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto-mono)",
					"lineHeight": "1.3"
				}
			}
		}
	},
	"templateParts": [
		{
			"name": "header",
			"area": "header",
			"title": "Header"
		},
		{
			"name": "footer",
			"area": "footer",
			"title": "Footer"
		}
	]

}